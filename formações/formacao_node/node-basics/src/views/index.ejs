<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>app</title>
  </head>
  <body>
    <div class="container">
      <div class="content">
        <% if (users.length > 0){ %> <% users.forEach(user => {%>
        <h1 class="title"><% user.firstName %></h>
        <h2 class="title"><% user.lastName %></h2>
        <p class="title"><% user.email %></>
        <p class="title"><% user.password %></>
          <% }) %> <% } else { %> 
            <p>Não há usuários para exibir</p>
            <% } %>
      </div>
    </div>
  </body>
</html>
